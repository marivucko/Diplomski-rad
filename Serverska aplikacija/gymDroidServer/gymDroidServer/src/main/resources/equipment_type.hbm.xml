<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gymdroid.domain.beans.EquipmentType" table="equipment_type">
        <id name="equipmentTypeId" type="int" column="equipmentTypeId">
            <generator class="native"/> 
        </id> 
        <property name="equipmentTypeName" column="equipmentTypeName" type="string"/>
        <property name="equipmentIsUpgradable" column="equipmentIsUpgradable" type="int"/>
        <property name="equipmentIsWeight" column="equipmentIsWeight" type="int"/>
        <property name="createdAt" column="createdAt" type="timestamp"/>
        <property name="updatedAt" column="updatedAt" type="timestamp"/>
        <property name="deletedAt" column="deletedAt" type="timestamp"/>
    </class>
</hibernate-mapping>
