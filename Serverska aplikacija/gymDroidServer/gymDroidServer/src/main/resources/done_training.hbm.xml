<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gymdroid.domain.beans.DoneTraining" table="done_training">
        <id name="doneTrainingId" type="int" column="doneTrainingId">
            <generator class="native"/>
        </id>
        <property name="doneTrainingAt" column="doneTrainingAt" type="timestamp"/>
        <property name="trainingId" column="trainingId" type="int"/>
        <property name="createdAt" column="createdAt" type="timestamp"/>
        <property name="updatedAt" column="updatedAt" type="timestamp"/>
        <property name="deletedAt" column="deletedAt" type="timestamp"/>
    </class>
</hibernate-mapping>
