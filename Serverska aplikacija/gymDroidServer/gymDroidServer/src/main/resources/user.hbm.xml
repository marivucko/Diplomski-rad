<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gymdroid.domain.beans.User" table="user">
        <id name="userId" type="int" column="userId">
            <generator class="native"/> 
        </id> 
        <property name="userEmail" column="userEmail" type="string"/>
        <property name="userPassword" column="userPassword" type="string"/>
        <property name="firebaseId" column="firebaseId" type="string"/>
        <property name="registrationType" column="registrationType" type="string"/>
        <property name="isActive" column="isActive" type="int"/>
        <property name="createdAt" column="createdAt" type="timestamp"/>
        <property name="updatedAt" column="updatedAt" type="timestamp"/>
        <property name="deletedAt" column="deletedAt" type="timestamp"/>
    </class>
</hibernate-mapping>
