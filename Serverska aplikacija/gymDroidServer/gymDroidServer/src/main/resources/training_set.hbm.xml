<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gymdroid.domain.beans.TrainingSet" table="training_set">
        <id name="trainingSetId" type="int" column="trainingSetId">
            <generator class="native"/>
        </id>
        <property name="trainingSetTime" column="trainingSetTime" type="long"/>
        <property name="trainingSetNumberOfReps" column="trainingSetNumberOfReps" type="int"/>
        <property name="trainingSetWeight" column="trainingSetWeight" type="double"/>
        <property name="trainingWorkoutId" column="trainingWorkoutId" type="int"/>
        <property name="createdAt" column="createdAt" type="timestamp"/>
        <property name="updatedAt" column="updatedAt" type="timestamp"/>
        <property name="deletedAt" column="deletedAt" type="timestamp"/>
    </class>
</hibernate-mapping>