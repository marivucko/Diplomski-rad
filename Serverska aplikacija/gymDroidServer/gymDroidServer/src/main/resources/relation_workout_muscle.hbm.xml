<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gymdroid.domain.beans.RelationWorkoutMuscle" table="relation_workout_muscle">
        <id name="relationId" type="int" column="relationId">
            <generator class="native"/> 
        </id> 
        <property name="workoutId" column="workoutId" type="int"/>
        <property name="muscleId" column="muscleId" type="int"/>
        <property name="muscleTargetPriority" column="muscleTargetPriority" type="int"/>
        <property name="createdAt" column="createdAt" type="timestamp"/>
        <property name="updatedAt" column="updatedAt" type="timestamp"/>
        <property name="deletedAt" column="deletedAt" type="timestamp"/>
    </class>
</hibernate-mapping>
